@article{samworth_optimal_2012,
	title = {Optimal weighted nearest neighbour classifiers},
	volume = {40},
	issn = {0090-5364, 2168-8966},
	url = {http://projecteuclid.org/euclid.aos/1359987536},
	doi = {10.1214/12-AOS1049},
	abstract = {We derive an asymptotic expansion for the excess risk (regret) of a weighted nearest-neighbour classifier. This allows us to find the asymptotically optimal vector of nonnegative weights, which has a rather simple form. We show that the ratio of the regret of this classifier to that of an unweighted kk-nearest neighbour classifier depends asymptotically only on the dimension dd of the feature vectors, and not on the underlying populations. The improvement is greatest when d=4d=4, but thereafter decreases as d→∞d{\textbackslash}rightarrow{\textbackslash}infty. The popular bagged nearest neighbour classifier can also be regarded as a weighted nearest neighbour classifier, and we show that its corresponding weights are somewhat suboptimal when dd is small (in particular, worse than those of the unweighted kk-nearest neighbour classifier when d=1d=1), but are close to optimal when dd is large. Finally, we argue that improvements in the rate of convergence are possible under stronger smoothness assumptions, provided we allow negative weights. Our findings are supported by an empirical performance comparison on both simulated and real data sets.},
	language = {EN},
	number = {5},
	urldate = {2015-04-27},
	journal = {The Annals of Statistics},
	author = {Samworth, Richard J.},
	month = oct,
